{
  "name": "tarojs-plugin-solid",
  "version": "0.0.1",
  "description": "taro插件，支持使用 Solid 开发小程序",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dev": "pnpm -r --filter=./packages/* run dev",
    "build": "pnpm -r --filter=./packages/* run build",
    "lint": "eslint packages/ --ext .js --ext .ts --ext .tsx",
    "edit-version": "node ./scripts/edit-version.js",
    "pub": "pnpm run edit-version && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phy-lei/tarojs-plugin-solid"
  },
  "keywords": [
    "taro",
    "taro3",
    "tarojs",
    "solid",
    "solidjs"
  ],
  "author": "phy-lei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phy-lei/tarojs-plugin-solid/issues"
  },
  "homepage": "https://github.com/phy-lei/tarojs-plugin-solid#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "volta": {
    "node": "18.17.1",
    "npm": "9.6.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.7.1",
    "typescript": "^5.3.3"
  }
}
